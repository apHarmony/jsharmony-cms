#jsharmony_cms_editor_bar {
  position:fixed;
  top:0px;
  left:0px;
  width:100%;
  background-color:#fff;
  box-shadow: 0px 3px 5px rgba(0,0,0,0.6);
  z-index:2147483640;
  font-family: 'Roboto', arial, helvetica, sans-serif !important;
  min-height:37px;
}
#jsharmony_cms_editor_bar .title {
  font-size:15px;
  padding-top:5px;
  padding-left:20px;
  line-height:26px;
  cursor:default;
  width:100%;
  overflow:hidden;
  white-space:nowrap;
  height:37px;
  border-bottom:1px solid #777;
  box-sizing:border-box;
}
#jsharmony_cms_editor_bar .actions {
  position:fixed;
  right:0px;
  background-color:#fff;
}
#jsharmony_cms_editor_bar .button {
  cursor:pointer;
  display:inline-block;
  font-size:15px;
  padding:8px 20px 7px 20px;
  /* border-left:1px solid #ccc; */
  border-left: 1px solid #333;
  color:#222;
  vertical-align:top;
  height:36px;
  box-sizing:border-box;
}
#jsharmony_cms_editor_bar a.button:hover {
  color:#888;
}
#jsharmony_cms_editor_bar a.button.selected {
  background-color:#f0f0f0;
}
#jsharmony_cms_editor_bar a.button.save.hasChanges {
  font-weight:bold;
}

#jsharmony_cms_editor_bar .button.icon {
  padding:4px 14px;
}
#jsharmony_cms_editor_bar .button.icon .material-icons {
  position:relative;
  top:4px;
  font-size:20px;
}
#jsharmony_cms_editor_bar .material-icons {
  font-family: 'Material Icons' !important;
}
#jsharmony_cms_editor_bar .autoHideEditorBar .hide { display:none; }
#jsharmony_cms_editor_bar .autoHideEditorBar .dock { display:inline-block; }
#jsharmony_cms_editor_bar .autoHideEditorBar.enabled .hide { display:inline-block; }
#jsharmony_cms_editor_bar .autoHideEditorBar.enabled .dock { display:none; }

#jsharmony_cms_editor_bar .page_settings {
  background-color: #fefefe;
  padding: 12px 20px;
  overflow-y:auto;
  box-sizing:border-box;
}

#jsharmony_cms_editor_bar .page_settings a.xtab {
  font-size:1.15em;
  background-color:transparent;
  border-top:none;
  border-right:none;
  border-left:none;
  border-bottom:2px solid transparent;
}
#jsharmony_cms_editor_bar .page_settings a.xtab.selected {
  border-bottom:2px solid black;
  color:#000;
}
#jsharmony_cms_editor_bar .page_settings .xtabcontrol .xpanel {
  border:none;
}
#jsharmony_cms_editor_bar .page_settings .xtabs {
}

#jsharmony_cms_editor_bar .page_settings textarea {
  padding:2px 5px;
  box-sizing:border-box;
  width:100%;
  height:50px;
  font-size:1.1em;
}

#jsharmony_cms_editor_bar .page_settings textarea.code {
  height:250px;
  font-size:1em;
  font-family: Consolas, Menlo, Monaco, Lucida Console, Liberation Mono, DejaVu Sans Mono, Bitstream Vera Sans Mono, Courier New, monospace, serif;
}

#jsharmony_cms_editor_bar .page_settings table { width:100%; }

#jsharmony_cms_editor_bar .page_settings td {
  padding-bottom:6px;
}

#jsharmony_cms_editor_bar .page_settings label {
  font-size:1.1em;
  white-space:nowrap;
  padding-right:6px;
}

#jsharmony_cms_editor_bar .page_settings input.page_settings_ctrl {
  box-sizing:border-box;
  width:100%;
  font-size:1.1em;
  padding:2px 5px;
}

#jsharmony_cms_editor_bar .page_settings .page_settings_tags_editor {
  width:100%;
}
#jsharmony_cms_editor_bar .page_settings .page_settings_tags_editor input {
  font-size:1.1em;
}



#jsharmony_cms_editor_bar .bluebutton {
  display:inline-block;
  padding:6px 20px;
  color:white;
  font-size:15px;
  letter-spacing:0.5px;
  background-color:#1a73e8;
  border-radius: 5px;
  margin-right:10px;
  margin-top:6px;
  margin-bottom:6px;
  font-weight:400;
}
#jsharmony_cms_editor_bar a.bluebutton:hover {
  background-color:#3c89ef;
}

#jsharmony_cms_body_toolbar {
  position:fixed;
  z-index: 9999;
  top:40px;
  display:none;
  width:100%;
}
#jsharmony_cms_body_toolbar .cke_top {
  padding-top:10px;
}
#jsharmony_cms_body:after {
  content: "";
  display: table;
  clear: both;
}