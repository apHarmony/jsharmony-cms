/*
z-index:
Editor Bar: 2147483640
Dialog Block: 2147483641 (So that validation errors show up over Editor Bar)
Loading Animation: 2147483642
*/
#jsharmony_cms_editor_bar {
  position:fixed;
  top:0px;
  left:0px;
  width:100%;
  background-color:#f9f9f9;
  box-shadow: 0px 2px 3px rgba(100,100,100,0.1);
  z-index:2147483640;
  font-family: 'Roboto', arial, helvetica, sans-serif !important;
  min-height:37px;
}
#jsharmony_cms_editor_bar .title {
  font-size:15px;
  padding-top:5px;
  padding-left:20px;
  line-height:26px;
  cursor:default;
  width:100%;
  overflow:hidden;
  white-space:nowrap;
  height:37px;
  border-bottom:1px solid #d2d2d2;
  box-sizing:border-box;
}
#jsharmony_cms_editor_bar .actions {
  position:fixed;
  right:0px;
  background-color:#f9f9f9;
}
#jsharmony_cms_editor_bar .button {
  cursor:pointer;
  display:inline-block;
  font-size:15px;
  padding:8px 20px 7px 20px;
  /* border-left:1px solid #ccc; */
  border-left: 1px solid #ddd;
  color:#222;
  vertical-align:top;
  height:36px;
  box-sizing:border-box;
}
#jsharmony_cms_editor_bar a.button:hover {
  color:#555;
}
#jsharmony_cms_editor_bar a.button.static:hover {
  color:#222;
  text-decoration:none;
  cursor:default;
}
#jsharmony_cms_editor_bar a.button.selected {
  background-color:#f0f0f0;
}
#jsharmony_cms_editor_bar a.button.save.hasChanges {
  font-weight:bold;
}

#jsharmony_cms_editor_bar .button.icon {
  padding:4px 14px;
}
#jsharmony_cms_editor_bar .button.icon .material-icons {
  position:relative;
  top:4px;
  font-size:20px;
}
#jsharmony_cms_editor_bar .button.texticon {
  position:relative;
  padding-left:42px;
}
#jsharmony_cms_editor_bar .button.texticon .material-icons {
  position:absolute;
  left:18px;
  top:7px;
  font-size:19px;
}
#jsharmony_cms_editor_bar .material-icons {
  font-family: 'Material Icons' !important;
}
#jsharmony_cms_editor_bar .autoHideEditorBar .hide { display:none; }
#jsharmony_cms_editor_bar .autoHideEditorBar .dock { display:inline-block; }
#jsharmony_cms_editor_bar .autoHideEditorBar.enabled .hide { display:inline-block; }
#jsharmony_cms_editor_bar .autoHideEditorBar.enabled .dock { display:none; }

#jsharmony_cms_editor_bar .page_settings {
  background-color: #fff;
  padding: 12px 20px;
  overflow-y:auto;
  box-sizing:border-box;
}

#jsharmony_cms_editor_bar .page_settings a.xtab {
  font-size:1.15em;
  background-color:transparent;
  border-top:none;
  border-right:none;
  border-left:none;
  border-bottom:2px solid transparent;
}
#jsharmony_cms_editor_bar .page_settings a.xtab.selected {
  border-bottom:2px solid #222;
  color:#222;
}
#jsharmony_cms_editor_bar .page_settings .xtabcontrol .xpanel {
  border:none;
}
#jsharmony_cms_editor_bar .page_settings .xtabs {
}

#jsharmony_cms_editor_bar .page_settings textarea {
  padding:2px 5px;
  box-sizing:border-box;
  width:100%;
  height:50px;
  font-size:1.1em;
}

#jsharmony_cms_editor_bar .page_settings textarea.code {
  height:250px;
  font-size:1em;
  font-family: Consolas, Menlo, Monaco, Lucida Console, Liberation Mono, DejaVu Sans Mono, Bitstream Vera Sans Mono, Courier New, monospace, serif;
}

#jsharmony_cms_editor_bar .page_settings table { width:100%; }

#jsharmony_cms_editor_bar .page_settings td {
  padding-bottom:6px;
}

#jsharmony_cms_editor_bar .page_settings label {
  font-size:1.1em;
  white-space:nowrap;
  padding-right:6px;
}

#jsharmony_cms_editor_bar .page_settings input.page_settings_ctrl {
  box-sizing:border-box;
  width:100%;
  font-size:1.1em;
  padding:2px 5px;
}

#jsharmony_cms_editor_bar .page_settings .page_settings_tags_editor {
  width:100%;
}

#jsharmony_cms_editor_bar .page_settings .page_settings_tags_editor input {
  font-size:1.1em;
}

#jsharmony_cms_editor_bar .jsharmony_cms_page_settings_properties .xbodyhead {
  padding-top: 0px;
}

#jsharmony_cms_editor_bar .jsharmony_cms_page_settings_properties .xform_caption {
  font-size:1.1em;
  padding-right:6px;
}

#jsharmony_cms_editor_bar .jsharmony_cms_page_settings_properties input {
  padding: 2px 5px;
  font-size:1.1em;
  margin-top:0px;
  margin-bottom:6px;
}

#jsharmony_cms_editor_bar .bluebutton {
  display:inline-block;
  padding:6px 20px;
  color:white;
  font-size:15px;
  letter-spacing:0.5px;
  background-color:#1a73e8;
  border-radius: 5px;
  margin-right:10px;
  margin-top:6px;
  margin-bottom:6px;
  font-weight:400;
}
#jsharmony_cms_editor_bar a.bluebutton:hover {
  background-color:#3c89ef;
}

body .tox-tinymce-inline {
  z-index: 9999;
}

body .tox-tinymce-aux {
  z-index: 10003;
}
body .tox-collection__item-icon .material-icons {
  font-family: 'Material Icons';
  font-size:25px;
}

body .tox .tox-collection__item-label h1,
body .tox .tox-collection__item-label h2,
body .tox .tox-collection__item-label h3,
body .tox .tox-collection__item-label h4,
body .tox .tox-collection__item-label h5,
body .tox .tox-collection__item-label h6 {
  line-height: 1;
  margin: 0;
  padding: 0;
}

#jsharmony_cms_content_editor_toolbar {
  position:fixed;
  z-index: 9999;
  top:40px;
  width:100%;
  opacity:0;
}

.jsharmony_cms_content:after {
  content: "";
  display: table;
  clear: both;
}

.mce-content-body:focus {
  outline: 2px solid #b4d7ff;
}

.jsharmony_cms_content.mce-content-body [contentEditable=false][data-mce-selected] {
  cursor: pointer;
  outline: 1px dashed #b4d7ff;
}

.jsharmony_cms_component_preview {
  border: 1px solid #eee;
}

.jsharmony_cms_component_preview [data-component-full-editor],
.jsharmony_cms_component_preview [data-component-title-editor] {
  min-width: 10px;
  min-height: 20px;
  outline: 1px solid #b4d7ff;
  outline-offset: -1px;
}

.jsharmony_cms_component.empty,
.jsharmony_cms_component_preview.empty {
  display: flex;
  flex-flow: column nowrap;
  justify-content: center;
  align-items: center;
  position: relative;
  background-color: #e2e2e2;
  border: 1px solid #999;
  padding:20px 20px 25px 20px;
}

.jsharmony_cms_component.empty h3,
.jsharmony_cms_component_preview.empty h3 {
  margin: 5px;
  font-weight: normal;
  color: #555;
  text-align:center;
}

.jsharmony_cms_component.empty h4,
.jsharmony_cms_component_preview.empty h4 {
  font-weight:normal;
  font-size:16px;
  margin-top:2px;
  text-align:center;
}

.jsharmony_cms_component.empty .material-icons,
.jsharmony_cms_component_preview.empty .material-icons {
  font-size: 48px;
  color: #555;
}

.xdialogblock .jsharmony_cms_component_dataGridEditor {
  padding-top:0px;
}

.jsharmony_cms_component_dataGridEditor > .xbodyhead {
  position:sticky;
  top:0px;
  z-index:1;
  border-bottom:2px solid #eee;
  background-color:#fff;
  padding-top:20px;
  padding-left:21px;
  padding-right:20px;
  padding-bottom:10px;
  margin-left:-20px;
  margin-right:-20px;
}

.jsharmony_cms_component_dataGridEditor .xform_label.updated[data-id="component_preview"] {
  background-color: inherit;
}

.jsharmony_cms_component_dataGridEditor [data-component-template="gridRow"] .material-icons {
  font-family: 'Material Icons' !important;
}

.jsharmony_cms_component_dataGridEditor [data-component-template="gridRow"] .toolbar {
  position: relative;
  text-align:right;
  margin-bottom:6px;
  margin-top:24px;
  padding-right:1px;
}

.jsharmony_cms_component_dataGridEditor [data-component-template="gridRow"] .toolbar button {
  outline: none;
  border: 0;
  background: inherit;
  cursor: pointer;
  font-size:19px;
}

.jsharmony_cms_component_dataGridEditor [data-component-template="gridRow"] .toolbar button[disabled] {
  opacity: 0.3;
}
.jsharmony_cms_component_dataGridEditor .xtbl_noresults .xgrid_norecords {
  display: none;
}
.jsharmony_cms_component_dataGridEditor .jsharmony_cms_component_preview {
  user-select: none;
}

.jsharmony_cms_component  .material-icons,
.jsharmony_cms_component_preview .material-icons {
  font-family: 'Material Icons' !important;
}

.jsharmony_cms_component_dialog input.xform_ctrl,
.jsharmony_cms_component_dialog select.xform_ctrl {
  margin-bottom:11px;
}

.jsharmony_cms_preview_editor {
  margin-top:5px;
  margin-bottom:2px;
}

.jsharmony_cms_preview_editor .jsharmony_cms_component_preview {
  border:1px solid #b4d7ff;
}

.xdialogblock {
  z-index:2147483641;
}

.xdialogblock .xdialogbox.jsharmony_cms_component_dialog .xbodyhead .xtitle {
  margin-top:0px;
}

.xdialogblock .xdialogbox.jsharmony_cms_component_dialog.jsharmony_cms_component_dataGridEditor .xbodyhead .xtitle {
  padding-bottom:0px;
}

.xdialogbox .xform_caption {
  font-size: 1.1em;
}

.xdialogbox .xform_caption.leading {
  padding-right: 6px;
}

.xdialogbox input.xform_ctrl,
.xdialogbox select.xform_ctrl  {
  box-sizing: border-box;
  font-size: 1.1em;
  padding: 2px 5px;
}

.xdialogbox input[type="button"] {
  background-color: #207ab7;
  background-image: none;
  background-position: none;
  background-repeat: none;
  border-color: #207ab7;
  border-radius: 3px;
  border-style: solid;
  border-width: 1px;
  box-shadow: none;
  box-sizing: border-box;
  color: #fff;
  cursor: pointer;
  display: inline-block;
  font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,Oxygen-Sans,Ubuntu,Cantarell,"Helvetica Neue",sans-serif;
  font-size: 14px;
  font-weight: 700;
  letter-spacing: 1;
  line-height: 24px;
  margin: 3px 0;
  outline: 0;
  padding: 4px 16px;
  text-align: center;
  text-decoration: none;
  text-transform: capitalize;
  white-space: nowrap;
}

.xdialogbox input.button_ok,
.xdialogbox input.button_no,
.xdialogbox input.button_cancel {
  margin-top:15px;
}

.xdialogbox input[type="button"]:hover:not(:disabled) {
  background-color: #1c6ca1;
  background-image: none;
  border-color: #1c6ca1;
  box-shadow: none;
  color: #fff;
}

.xdialogbox input[type="button"].secondary {
  background-color: #f0f0f0;
  background-image: none;
  background-position: none;
  background-repeat: none;
  border-color: #f0f0f0;
  border-radius: 3px;
  border-style: solid;
  border-width: 1px;
  box-shadow: none;
  color: #222f3e;
}

.xdialogbox input[type="button"].secondary:hover:not(:disabled) {
  background-color: #e3e3e3;
  background-image: none;
  border-color: #e3e3e3;
  box-shadow: none;
  color: #222f3e;
}

.xdialogbox .xbodyhead .xtitle {
  font-size: 20px;
  font-style: normal;
  font-weight: 400;
  line-height: 1.3;
  margin-bottom: 0px;
  margin-top: 10px;
}
.xdialogblock .jsharmony_cms_content_editor_toolbar .tox-dialog-wrap {
  width: 100vw;
  height: 100vh;
}
