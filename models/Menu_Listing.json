{
  "Menu_Listing": {
    "layout":"grid",
    "table":"{schema}.v_my_menu",
    "actions":"BUD",
    "roles": {"AUTHOR": "BIUD","PUBLISHER": "BIUD","VIEWER":"B"},
    "caption":["Menu","Menus"],
    "title":"Menus",
    "reselectafteredit": "row",
    "sort":["^menu_name"],
    "menu":"jsHarmonyCMS/Menu_Listing",
    "breadcrumbs":{
      "title": "<div class='bcrumbs_branch_header'>Branch</div><div class='bcrumbs_branch_body'><#=bcrumbs.branch_desc#></div>",
      "sql": "select branch_desc, deployment_target_params from {schema}.v_my_branch_desc left outer join {schema}.v_my_site on v_my_site.site_id = v_my_branch_desc.site_id where branch_id={schema}.my_current_branch_id()"
    },
    "oninit": "xmodel.controller.grid.OnLoadError = function(err){ if(err && err.Number==-14){ XExt.Alert('Please checkout a branch', function(){ XExt.navTo(jsh._BASEURL+xmodel.module_namespace+'Branch_Active_Listing', { force: true }) }); return true; } }",
    "fields": [
      {"name":"media_key","caption":"Key","control":"hidden"},
      {"name":"media_desc","caption":"Title","control":"textbox_M"},
      {"name":"media_path","caption":"Media Path","control":"textbox_L"},
      {"name":"media_type","caption":"Type"},
      {"name":"media_tags","caption":"Tags","control":"label"},
      {"name":"media_size","caption":"Size","control":"label","format":"bytes","cellstyle":"white-space:nowrap;"},
      {"name":"media_width","caption":"Width","control":"label"},
      {"name":"media_height","caption":"Height","control":"label"},
      {"name":"media_mtstmp","caption":"Modified","control":"label","actions":"B","format":["date","MM/DD/YYYY h:mm a"],"cellstyle":"white-space:nowrap;"},
      {"control":"html","cellstyle":"text-align:center;","value":"<a class='xgrid_action' href='#' onclick='<%=jsh._instance%>.getFileProxy().prop(\"src\",\"<%=jsh._BASEURL%>_funcs/media/<#=data.media_key#>/?download\");'><img height='14' src='/images/icon_download.png' /></a>"},
    ]
  }
}