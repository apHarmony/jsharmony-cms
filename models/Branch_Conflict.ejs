        <input type="button" value="Unresolve" class="button_unresolve_sitemap" data-sitemap_key="<#=branch_sitemap.sitemap_key#>"  data-sitemap_id="<#=null#>" data-branch_sitemap_action="<#=null#>" />
      </h4>
    <# } else { #>
      <div class="branch_diff_comparison">
        <div class="branch_diff_comparison_src">
          <#- include('Sitemap_Diff_Comparison', {
            branch_sitemap_action: branch_sitemap.src_branch_sitemap_action,
            sitemap_key: branch_sitemap.sitemap_key,
            title: 'Source',
            diff: branch_sitemap.src_diff,
            map: map,
            new_sitemap: branch_sitemap.src_sitemap,
            old_sitemap: branch_sitemap.src_orig_sitemap}) #>
        </div>

        <div class="branch_diff_comparison_dst">
          <#- include('Sitemap_Diff_Comparison', {
            branch_sitemap_action: branch_sitemap.dst_branch_sitemap_action,
            sitemap_key: branch_sitemap.sitemap_key,
            title: 'Destination',
            diff: branch_sitemap.dst_diff,
            map: map,
            new_sitemap: branch_sitemap.dst_sitemap,
            old_sitemap: branch_sitemap.dst_orig_sitemap}) #>
        </div>
      </div>
    <# } #>

  <# }); #>

  <hr/>

  <# if (branch_diff.unresolved > 0) { #>
    <h3>Unresolved Conflicts: <#= branch_diff.unresolved #></h3>
  <# } else { #>
    <h3>All Conflicts Resolved</h3>
    <input type="button" value="Merge" class="button_execute_merge" style="margin-right:15px;" />
  <# } #>
